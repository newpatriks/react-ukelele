{
  "name": "react-ukelele",
  "type": "module",
  "version": "0.4.5",
  "description": "Small add-on to add svg ukelele chords into your app",
  "keywords": [
    "ukelele",
    "svg",
    "music",
    "chords"
  ],
  "homepage": "https://github.com/newpatriks/ukelele",
  "license": "MIT",
  "author": {
    "name": "Jordi Llobet",
    "url": "http://jllobet.me"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/react-ukelele.umd.cjs",
  "module": "./dist/react-ukelele.js",
  "exports": {
    ".": {
      "import": "./dist/react-ukelele.js",
      "require": "./dist/react-ukelele.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "type-check": "tsc -p tsconfig.typecheck.json",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newpatriks/ukelele"
  },
  "dependencies": {
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vitest": "^0.34.6",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "jsdom": "^22.1.0"
  }
}
